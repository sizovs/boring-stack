{
  "compilerOptions": {
    "paths": {
      "/static/*": ["./static/*"],
      "#modules/*": [
        "./modules/*.js",
      ],
      "#application/*": [
        "./application/*.js",
      ],
      "#tests/*": [
        "./tests/*.js"
      ],
      "#root/*": [
        "./*.js",
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
